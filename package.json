{
  "activationEvents": [
    "onLanguage:shellscript",
    "workspaceContains:**/*.ebuild"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "pkgcheck.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "dependencies": {
    "execa": "^5.0.0",
    "ramda": "^0.29.0"
  },
  "description": "",
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/ramda": "^0.29.2",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "displayName": "pkgcheck",
  "engines": {
    "vscode": "^1.79.0"
  },
  "main": "./out/extension.js",
  "name": "pkgcheck",
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "yarn run compile",
    "watch": "tsc -watch -p ./"
  },
  "version": "0.0.1"
}
